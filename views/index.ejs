<html lang="en">
  <head>
    <title>Todo List</title>
    <!-- <script
      src="https://code.jquery.com/jquery-1.12.0.min.js"
      integrity="sha256-Xxq2X+KtazgaGuA2cWR1v3jJsuMJUozyIXDB3e793L8="
      crossorigin="anonymous"
    ></script> -->
    <!-- Compiled and minified CSS -->
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css"
    />

    <link rel="stylesheet" type="text/css" href="/assets/styles.css" />
  </head>
  <body>
    <div id="todo-table row">
      <form method="post" class="z-depth-2">
        <h3 class="center">Cadastro do jogador UOL</h3>

        <div>
          <label for="fname">Nome:</label>
          <input
            type="text"
            id="name"
            name="name"
            placeholder="name..."
            required
          />
        </div>

        <div>
          <span class="my-email hide">Este email ja foi cadastrado</span><br />
          <label for="femail">E-mail: </label>
          <input
            type="email"
            id="email"
            name="email"
            placeholder="email..."
            required
          />
        </div>

        <div>
          <span class="my-telephone hide">Numero insuficiente</span><br />

          <label for="ftelephone">Telephone</label>
          <input
            placeholder="00-0000-0000"
            id="telephone"
            type="tel"
            class="validate"
            maxlength="10"
            required
          />
        </div>

        <span class="my-group hide">Escolha um grupo</span><br />
        <label class="name-group">Quero ser do grupo:</label>

        <div class="div-checkbox">
          <p>
            <label>
              <input type="checkbox" id="lj" />
              <span>Quero ser da Liga da Justi√ßa</span>
            </label>
          </p>

          <p>
            <label>
              <input type="checkbox" id="avangers" />
              <span>Quero ser dos Vingadores</span>
            </label>
          </p>
        </div>

        <div class="div-btn center">
          <button
            class="waves-effect #66bb6a green lighten-1 btn-large"
            id="cadaster"
            type="submit"
          >
            Cadastrar
          </button>
          <a
            class="waves-effect #4fc3f7 light-blue lighten-2 btn-large"
            id="list-cadaster"
            >Listar jogadores</a
          >
        </div>
      </form>
    </div>
    <script type="module" language="JavaScript" src="/assets/main.js"></script>
  </body>
</html>
